<h1>Listing users</h1>

<table>
  <tr>
    <th>User name</th>
    <th>Created On</th>
  </tr>

<% @users.each do |user| %>
  <tr>
    <td><%=h user.user_name %></td>
    <td><%=h user.created_at %></td>
    <td><%= link_to 'Edit', edit_user_path(user) %></td>
    <td><%= link_to 'Destroy', user, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New user', new_user_path %>
