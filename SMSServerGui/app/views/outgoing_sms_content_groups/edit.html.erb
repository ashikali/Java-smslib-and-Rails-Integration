<h1>Editing outgoing_sms_content_group</h1>

<% form_for(@outgoing_sms_content_group) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :group_name %><br />
    <%= f.text_field :group_name %>
  </p>
  <p>
    <%= f.label :sms_content %><br />
    <%= f.text_area :sms_content %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @outgoing_sms_content_group %> |
<%= link_to 'Back', outgoing_sms_content_groups_path %>